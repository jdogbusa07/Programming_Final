@page "{id:int}"
@model EditModel

<h1>Edit Rentals</h1>

<form method="post">
    <input type="hidden" asp-for="Rent.RentID" />
    <div class="form-group">
        <label asp-for="Rent.RentDate"></label>
        <input asp-for="Rent.RentDate" class="form-control" />
        <span asp-validation-for="Rent.RentDate" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
    <a asp-page="Index">Cancel</a>
</form>
